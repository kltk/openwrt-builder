# 3. luci
# Web
# https for uhttpd
# CONFIG_PACKAGE_luci-ssl-openssl=y
CONFIG_PACKAGE_luci-ssl=y

# nginx for luci
CONFIG_PACKAGE_luci-nginx=m
CONFIG_PACKAGE_luci-ssl-nginx=m
CONFIG_PACKAGE_nginx-all-module=m

# nginx
CONFIG_NGINX_DAV=y
CONFIG_NGINX_HTTP_V2=y

# Luci Applications
CONFIG_PACKAGE_luci-app-aria2=m
CONFIG_PACKAGE_luci-app-cpufreq=m
CONFIG_PACKAGE_luci-app-hd-idle=m
CONFIG_PACKAGE_luci-app-minidlna=m
CONFIG_PACKAGE_luci-app-smartdns=m
CONFIG_PACKAGE_luci-app-travelmate=m
CONFIG_PACKAGE_luci-app-upnp=m
